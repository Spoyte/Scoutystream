{
  "name": "scoutystream",
  "private": true,
  "packageManager": "pnpm@9",
  "workspaces": [
    "apps/*",
    "contracts/*"
  ],
  "scripts": {
    "dev": "pnpm --parallel --filter \"./apps/*\" dev",
    "build": "pnpm --filter \"./apps/*\" build",
    "lint": "pnpm --parallel --filter \"./apps/*\" lint",
    "typecheck": "pnpm --parallel --filter \"./apps/*\" typecheck",
    "test": "echo 'No tests configured yet'"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20"
  }
}
